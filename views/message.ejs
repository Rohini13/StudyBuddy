<div id="messages" style="text-align: center; z-index: 99; width:400px; position:fixed; top:2px; left: 450px">
    <% Object.keys(messages).forEach(function (type) { %>

            <% console.log(type);messages[type].forEach(function (message) { %>
                <%if(type=='error'){%>
                        <div class="alert alert-<%= type %>" style="z-index: 99; color: white; background-color: #bc3a3a"><%= message %></div>
                        <%}else{%>
                        <div class="alert alert-<%= type %>" style="z-index: 99;"><%= message %></div>
                <%}%>
            <% }) %>
    <% }) %>
</div>